<script>
    var a = $.session.get('key');
    var b = "#li"+a;
    var c = $.session.get('keys');
    var d = "#dd"+c;
    console.log(b);
    console.log(d);
    function sessionid(id){
        $.session.set('key', id);
    }
    function sessionidd(id){
        $.session.set('keys', id);
    }
    $(b).addClass("open");
    $(d).css("background","#2d363f");
    $(".tabchange li dd").click(function () {
        var name=$(this).find("a").html();
        var to = "<?php echo e(csrf_token()); ?>"
        $.ajax({
            url:"{('Index/getName')}",
            type:"post",
            data:{'_token': to,"name":name},
            dataType:"json",
            async:false,
            success:function (result) {
                alert(result);
            }
        });
    });
    function Clearcache(obj) {
        layer.confirm('确定要清除缓存么吗？', {
            btn: ['确定', '我在想想'] //可以无限个按钮
            ,yes: function(index, layero){
                var url = obj.href;
                var to = "<?php echo e(csrf_token()); ?>";
                $.ajax({
                    url:url,
                    data: {},
                    type:'get',
                    success:function (result) {
                        //判断result结果
                        if (result.error){
                            layer.msg(result.msg, {icon: 6});
                            window.location.reload();
                        }else{
                            layer.msg(result.msg, {icon: 5});
                        }
                    }
                })
            }
        }, function(index){
            return false;
        });
    }
    //        提示框Start
    if($(".sec .al").text()!=""){
        if ($(".sec p").text()==1 || $(".sec .al").text()==0){
            layer.msg($(".sec .al").text(), {icon: 6});
        }else{
            layer.msg($(".sec .al").text(), {icon: 5});
        }
    }
    //        提示框Over
    // 修改状态Start
    function changeStatus(obj,id,url){
        var num=$(obj).attr("num");
        if(num==1){
            num = 0;
        }else{
            num = 1;
        }
        $.ajax({
            url:url,
            type:"post",
            data:{'_token':"<?php echo e(csrf_token()); ?>",'num':num,'game_id':id},
            dataType:"json",
            async:false,
            success:function (result) {
                if (result == 1){
                    layer.msg("更新状态成功",{icon:6,time:1000});
                    location.reload();
                }else{
                    layer.msg("更新状态失败",{icon:5,time:1000});
                }
            }
        });
    }
    // 修改状态Over
//    排序Start

//    排序Over
</script>
